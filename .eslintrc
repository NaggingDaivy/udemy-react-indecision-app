{
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ] ,
    "parserOptions": {
        "ecmaVersion": 6 
        
    },
    "env": {
        "node":true,
        "browser": true,
        "es6": true
    },
    "rules" : {
        "comma-dangle":"warn",
        "react/jsx-filename-extension":0,
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "indent": ["error", 4]

    }
   
    
}
